<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl plugin="mailer@1.16">
  <smtpAuthUsername>{{ jenkins_mailer_username }}</smtpAuthUsername>
  <smtpAuthPassword>{{ jenkins_mailer_password }}</smtpAuthPassword>
  <replyToAddress>{{ jenkins_mailer_username }}</replyToAddress>
  <smtpHost>{{ jenkins_mailer_smtp_host }}</smtpHost>
  <useSsl>{{ jenkins_mailer_smtp_ssl }}</useSsl>
  <smtpPort>{{ jenkins_mailer_smtp_port }}</smtpPort>
  <charset>UTF-8</charset>
</hudson.tasks.Mailer_-DescriptorImpl>