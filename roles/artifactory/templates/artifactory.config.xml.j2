<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://artifactory.jfrog.org/xsd/1.8.1"
        xsi:schemaLocation="http://artifactory.jfrog.org/xsd/1.8.1
        http://www.jfrog.org/xsd/artifactory-v1_8_1.xsd">
    <serverName>{{ artifactory_server_name }}</serverName>
    <offlineMode>false</offlineMode>
    <helpLinksEnabled>true</helpLinksEnabled>
    <fileUploadMaxSizeMb>100</fileUploadMaxSizeMb>
    <dateFormat>dd-MM-yy HH:mm:ss z</dateFormat>
    <addons>
        <showAddonsInfo>true</showAddonsInfo>
        <showAddonsInfoCookie>1453090201198</showAddonsInfoCookie>
    </addons>
    <mailServer>
        <enabled>true</enabled>
        <host>{{ artifactory_mail_host }}</host>
        <port>{{ artifactory_mail_port }}</port>
        <username>{{ artifactory_mail_username }}</username>
        <password>{{ artifactory_mail_password }}</password>
        <from>{{ artifactory_mail_from }}</from>
        <subjectPrefix>{{ artifactory_mail_subject_prefix }}</subjectPrefix>
        <tls>false</tls>
        <ssl>false</ssl>
        <artifactoryUrl>{{ artifactory_mail_url }}</artifactoryUrl>
    </mailServer>
    <bintrayConfig>
        <fileUploadLimit>0</fileUploadLimit>
    </bintrayConfig>
    <security>
        <anonAccessEnabled>true</anonAccessEnabled>
        <anonAccessToBuildInfosDisabled>false</anonAccessToBuildInfosDisabled>
        <hideUnauthorizedResources>false</hideUnauthorizedResources>
        <passwordSettings>
            <encryptionPolicy>supported</encryptionPolicy>
            <expirationPolicy>
                <enabled>false</enabled>
                <passwordMaxAge>60</passwordMaxAge>
                <notifyByEmail>true</notifyByEmail>
            </expirationPolicy>
        </passwordSettings>
        <ldapSettings/>
        <ldapGroupSettings/>
        <userLockPolicy>
            <enabled>false</enabled>
            <loginAttempts>5</loginAttempts>
        </userLockPolicy>
    </security>
    <backups>
        <backup>
            <key>backup-daily</key>
            <enabled>true</enabled>
            <cronExp>0 0 2 ? * MON-FRI</cronExp>
            <retentionPeriodHours>0</retentionPeriodHours>
            <createArchive>false</createArchive>
            <excludedRepositories>
                <repositoryRef>jcenter</repositoryRef>
            </excludedRepositories>
            <sendMailOnError>true</sendMailOnError>
            <excludeBuilds>false</excludeBuilds>
            <excludeNewRepositories>false</excludeNewRepositories>
        </backup>
        <backup>
            <key>backup-weekly</key>
            <enabled>false</enabled>
            <cronExp>0 0 2 ? * SAT</cronExp>
            <retentionPeriodHours>336</retentionPeriodHours>
            <createArchive>false</createArchive>
            <excludedRepositories>
                <repositoryRef>jcenter</repositoryRef>
            </excludedRepositories>
            <sendMailOnError>true</sendMailOnError>
            <excludeBuilds>false</excludeBuilds>
            <excludeNewRepositories>false</excludeNewRepositories>
        </backup>
    </backups>
    <indexer>
        <enabled>true</enabled>
        <cronExp>0 23 5 * * ?</cronExp>
        <includedRepositories>
            <repositoryRef>STRIVE</repositoryRef>
            <repositoryRef>central.secure</repositoryRef>
            <repositoryRef>gradle</repositoryRef>
            <repositoryRef>gradle.plugins</repositoryRef>
            <repositoryRef>jboss.repo</repositoryRef>
            <repositoryRef>jcenter</repositoryRef>
            <repositoryRef>mulesoft.releases</repositoryRef>
            <repositoryRef>sonatype</repositoryRef>
            <repositoryRef>spring.milestone</repositoryRef>
            <repositoryRef>spring.release</repositoryRef>
        </includedRepositories>
    </indexer>
    <localRepositories>
        <localRepository>
            <key>{{ artifactory_local_repository_key }}</key>
            <type>maven</type>
            <includesPattern>**/*</includesPattern>
            <repoLayoutRef>maven-2-default</repoLayoutRef>
            <dockerApiVersion>V2</dockerApiVersion>
